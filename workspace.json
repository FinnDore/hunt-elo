{
  "version": 2,
  "projects": {
    "hunt-elo": "apps/hunt-elo",
    "hunt-elo-e2e": "apps/hunt-elo-e2e",
    "utils": "libs/utils",
    "workspace": {
      "root": ".",
      "targets": {
        "version": {
          "executor": "@jscutlery/semver:version",
          "options": {
            "syncVersions": true,
            "commitMessageFormat": "chore(${projectName}): release version ${version}"
          }
        }
      }
    }
  }
}
